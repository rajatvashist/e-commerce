<div id="page-wrapper" >
    <div id="page-inner">
        <div class="row">
            <div class="col-md-12">
             <h2>Blank Page</h2>   
             <% if(q==1) { %>
              <div class="alert alert-success">
                <h2>Product Added Successfully</h2>
              </div>
              <% } %>

            </div>
        </div>
         <!-- /. ROW  -->
         <hr />

         <form action="/admin/addproduct" method="post" enctype="multipart/form-data">
            <div class="form-group">
              <label for="name">Product name:</label>
              <input type="text" class="form-control" id="name" placeholder="Add product" name="productname">
            </div>

            <div class="form-group">
              <label for="image">Image:</label>
              <input type="file" class="form-control" id="image" placeholder="Add product" name="productimage">
            </div>
      
            <div class="form-group">
              <label for="price">Price:</label>
              <input type="text" class="form-control" id="price" placeholder="Add price" name="productprice">
            </div>
            
            <button type="submit" class="btn btn-default">Submit</button>
          </form>




       
</div>
     <!-- /. PAGE INNER  -->
    </div>
 <!-- /. PAGE WRAPPER  -->
</div>
<!-- /. WRAPPER  -->
<!-- SCRIPTS -AT THE BOTOM TO REDUCE THE LOAD TIME-->
