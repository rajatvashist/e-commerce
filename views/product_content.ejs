<section class="product_section">
  <div class="container">
    <div class="product_heading">
      <h2>Latest Watches</h2>
    </div>
    <div class="product_container">
      <% for(let i of d) {%>
      <div class="box">
        <div class="box-content">
          <div class="img-box">
            <img src="<%=i.productimage%>" alt="" />
          </div>
          <div class="detail-box">
            <div class="text">
              <h6><%=i.productname%></h6>
              <h5><span>â‚¹</span><%=i.productprice%></h5>
            </div>
            <div class="like">
              <h6>Like</h6>
              <div class="star_container">
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
                <i class="fa fa-star" aria-hidden="true"></i>
              </div>
            </div>
          </div>
        </div>
        <div class="btn-box">
          <a href="/product/<%=i._id%>"> Add To Cart </a>
        </div>
      </div>
      <%}%>

      <!-- end product section -->
    </div>
  </div>
</section>
